{
  "name": "BluetoothPeripheral",
  "version": "0.1.9",
  "summary": "Bluetooth peripheral",
  "homepage": "https://github.com/sheehanmunim/react-native-bluetooth-peripheral#readme",
  "license": "MIT",
  "authors": "sheehanmunim <sheehanmunim@gmail.com> (https://github.com/sheehanmunim)",
  "platforms": {
    "ios": "15.1"
  },
  "source": {
    "git": "https://github.com/sheehanmunim/react-native-bluetooth-peripheral.git",
    "tag": "0.1.9"
  },
  "source_files": "ios/**/*.{h,m,mm,cpp}",
  "private_header_files": "ios/**/*.h",
  "compiler_flags": "-DRCT_NEW_ARCH_ENABLED=1 -DFOLLY_NO_CONFIG -DFOLLY_MOBILE=1 -DFOLLY_USE_LIBCPP=1 -DFOLLY_CFG_NO_COROUTINES=1 -DFOLLY_HAVE_CLOCK_GETTIME=1 -Wno-comma -Wno-shorten-64-to-32",
  "dependencies": {
    "React-Core": [],
    "RCT-Folly": [
      "2024.11.18.00"
    ],
    "glog": [],
    "React-RCTFabric": [],
    "ReactCodegen": [],
    "RCTRequired": [],
    "RCTTypeSafety": [],
    "ReactCommon/turbomodule/bridging": [],
    "ReactCommon/turbomodule/core": [],
    "React-NativeModulesApple": [],
    "Yoga": [],
    "React-Fabric": [],
    "React-graphics": [],
    "React-utils": [],
    "React-featureflags": [],
    "React-debug": [],
    "React-ImageManager": [],
    "React-rendererdebug": [],
    "DoubleConversion": [],
    "React-jsi": [],
    "React-renderercss": [],
    "hermes-engine": [],
    "React-hermes": []
  },
  "pod_target_xcconfig": {
    "HEADER_SEARCH_PATHS": "\"$(PODS_ROOT)/boost\" \"$(PODS_ROOT)/Headers/Private/Yoga\"",
    "CLANG_CXX_LANGUAGE_STANDARD": "c++20",
    "OTHER_CPLUSPLUSFLAGS": "$(inherited) -DRCT_NEW_ARCH_ENABLED=1 -DFOLLY_NO_CONFIG -DFOLLY_MOBILE=1 -DFOLLY_USE_LIBCPP=1 -DFOLLY_CFG_NO_COROUTINES=1 -DFOLLY_HAVE_CLOCK_GETTIME=1 -Wno-comma -Wno-shorten-64-to-32",
    "OTHER_SWIFT_FLAGS": "$(inherited) -DRCT_NEW_ARCH_ENABLED"
  }
}
